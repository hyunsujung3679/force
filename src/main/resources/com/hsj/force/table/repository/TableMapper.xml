<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hsj.force.table.repository.TableMapper">

    <select id="selectTableNotExistOrderList" resultType="com.hsj.force.domain.dto.TableListDTO">
        SELECT T.TABLE_NO
             , T.TABLE_NAME
          FROM TTABLE T
         WHERE NOT EXISTS (SELECT *
                             FROM TORDER O
                            WHERE T.TABLE_NO = O.TABLE_NO
                              AND O.ORDER_STATUS_NO = 'OS001'
        )
         ORDER BY T.TABLE_NO ASC
    </select>

</mapper>